{
  "root": "People",
  "structure": [
    {
      "name": "Name",
      "patterns": [ "^[A-Z][a-z]+\\s[A-Z][a-z]+$" ],
      "min_length": 3,
      "validation_regex": "^[A-Z][a-z]+\\s[A-Z][a-z]+$"
    },
    {
      "name": "PhoneNumber",
      "patterns": [ "^\\+1\\s\\d{3}\\s\\d{3}\\s\\d{4}$", "^\\+1\\d{10}$" ],
      "min_length": 10,
      "validation_regex": "^\\+1\\s\\d{3}\\s\\d{3}\\s\\d{4}$"
    },
    {
      "name": "Email",
      "patterns": [ "^[\\w-.]+@([\\w-]+\\.)+[\\w-]{2,4}$" ],
      "min_length": 5,
      "validation_regex": "^[\\w-.]+@([\\w-]+\\.)+[\\w-]{2,4}$"
    },
    {
      "name": "Date",
      "patterns": [ "^\\d{4}-\\d{2}-\\d{2}$" ],
      "min_length": 10,
      "validation_regex": "^\\d{4}-\\d{2}-\\d{2}$"
    }
  ]
}
